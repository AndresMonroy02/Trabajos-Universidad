import random 
lista= []
C=0
N=int(input("Digite el TamaÃ±o del arreglo: "))
for i in range (N):
    lista.append(random.randint(1,100))
print(lista)
for i in range(N):
    for j in range(N):
        C=lista[i]
        if lista[i]<lista[j]:
           lista[i]=lista[j]
           lista[j]=C   
print("Lista Organizada: ")
print(lista)

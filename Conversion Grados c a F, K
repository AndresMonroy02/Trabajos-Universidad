temp_celsius=int(input("digite temperatura en grados celsius:"))
temp_farenheit=(9.0/5.0*temp_celsius)+32
temp_kelvin=(temp_celsius+273.15)
print ("Temperatura en celsius es:","{:.2f}".format(temp_celsius))
print("Temperatura en farenheit es:","{:.2f}".format(temp_farenheit))
print("Temperatura en kelvin es:","{:.3f}".format(temp_kelvin))
